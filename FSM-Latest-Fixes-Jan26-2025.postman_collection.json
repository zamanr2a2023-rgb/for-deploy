				{
					"name": "Get Call Center Dashboard Stats",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Call center stats retrieved successfully', () => {",
									"    pm.expect(pm.response.code).to.equal(200);",
									"    const response = pm.response.json();",
									"    pm.expect(response.success).to.be.true;",
									"    pm.expect(response.data).to.have.property('serviceRequests');",
									"    pm.expect(response.data).to.have.property('workOrders');",
									"    pm.expect(response.data).to.have.property('summary');",
									"});",
									"",
									"pm.test('Service request stats have correct structure', () => {",
									"    const response = pm.response.json();",
									"    const sr = response.data.serviceRequests;",
									"    pm.expect(sr).to.have.property('pending');",
									"    pm.expect(sr).to.have.property('dispatched');",
									"    pm.expect(sr).to.have.property('completed');",
									"    pm.expect(sr).to.have.property('cancelled');",
									"});",
									"",
									"pm.test('Work order stats have correct structure', () => {",
									"    const response = pm.response.json();",
									"    const wo = response.data.workOrders;",
									"    pm.expect(wo).to.have.property('assigned');",
									"    pm.expect(wo).to.have.property('inProgress');",
									"    pm.expect(wo).to.have.property('completed');",
									"    pm.expect(wo).to.have.property('cancelled');",
									"});"
								],
